<article id="<%= page.slug %>" class="article article-type-<%= page.layout %>">
  <div class="article-inner">
    <header class="article-header">
      <% if (page.title){ %>
        <h1 class="article-title" itemprop="name">
          <%= page.title %>
        </h1>
      <% } %>
    </header>
    <div class="article-entry">
      <%- page.content %>
    </div>
    <footer class="article-footer">
    </footer>
  </div>
</article>